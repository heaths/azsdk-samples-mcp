{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "useGitignore": true,
  "ignorePaths": [
    ".vscode/",
    "infra/",
    "tests/**/Content/",
    ".cspell.json"
  ],
  "words": [
    "accessibilities",
    "appconfig",
    "azsdk",
    "postbuild",
    "postprovision",
    "pwsh",
    "rethrowing",
    "serde"
  ],
  "overrides": [
    {
      "filename": "**/tsconfig.json",
      "words": [
        "nodenext"
      ]
    }
  ]
}